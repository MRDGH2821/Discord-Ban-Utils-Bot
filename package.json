{
  "name": "discord-ban-utils-bot",
  "version": "3.0.4",
  "private": true,
  "description": "Discord Ban Utilities for Mutual Servers!",
  "keywords": [
    "discord",
    "ban",
    "utilities"
  ],
  "homepage": "https://github.com/MRDGH2821/Discord-Ban-Utils-Bot#readme",
  "bugs": {
    "url": "https://github.com/MRDGH2821/Discord-Ban-Utils-Bot/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MRDGH2821/Discord-Ban-Utils-Bot.git"
  },
  "license": "MIT",
  "author": {
    "name": "MRDGH2821",
    "url": "https://github.com/MRDGH2821"
  },
  "contributors": [
    {
      "name": "AroLeaf",
      "url": "https://github.com/AroLeaf"
    }
  ],
  "scripts": {
    "prebuild": "npm run generate:loaders",
    "build": "tsc",
    "dev": "npm run build && npm run dev:start",
    "dev:start": "node --env-file .env dist/index.js",
    "format": "prettier --write .",
    "format-lint": "npm run format && npm run lint",
    "generate": "sapphire generate",
    "generate:loaders": "node ./buildLibs/generate-virtual-piece-loader.mjs",
    "lint": "eslint --fix . --ignore-path .gitignore",
    "mega-lint": "npx -y mega-linter-runner --flavor javascript",
    "prepare": "is-ci || husky install",
    "start": "node dist/index.js",
    "prewatch": "npm run generate:loaders",
    "watch": "tsc -w",
    "prewatch:start": "npm run generate:loaders",
    "watch:start": "tsc-watch --onSuccess \"npm run dev:start\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --fix --ignore-path .gitignore"
    ],
    "*": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@sapphire/decorators": "^6.0.3",
    "@sapphire/discord.js-utilities": "^7.1.3",
    "@sapphire/framework": "^5.0.2",
    "@sapphire/plugin-logger": "^4.0.1",
    "@sapphire/plugin-subcommands": "^6.0.1",
    "@sapphire/shapeshift": "^3.9.4",
    "@sapphire/time-utilities": "^1.7.11",
    "@sapphire/utilities": "^3.14.0",
    "colorette": "^2.0.20",
    "discord.js": "^14.14.1",
    "dpaste-ts": "^3.0.0",
    "enhanced-ms": "^2.3.0",
    "firebase-admin": "^11.11.1",
    "markdown-table": "^3.0.3",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@sapphire/cli": "^1.9.1",
    "@sapphire/ts-config": "^5.0.0",
    "@types/node": "^20.9.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "bun-types": "latest",
    "dotenv": "^16.3.1",
    "eslint": "^8.55.0",
    "eslint-config-adjunct": "^4.12.2",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-auto": "^0.7.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-array-func": "^4.0.0",
    "eslint-plugin-const-case": "^1.2.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-pii": "^1.0.2",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-unicorn": "^49.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-xss": "^0.1.12",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "prettier-config-mrdgh2821": "^3.0.0",
    "prettier-plugin-packagejson": "^2.4.7",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=20.6.0"
  },
  "engineStrict": true
}