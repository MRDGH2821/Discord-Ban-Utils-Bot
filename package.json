{
  "name": "discord-ban-utils-bot",
  "version": "3.0.4",
  "private": true,
  "description": "Discord Ban Utilities for Mutual Servers!",
  "keywords": [
    "discord",
    "ban",
    "utilities"
  ],
  "homepage": "https://github.com/MRDGH2821/Discord-Ban-Utils-Bot#readme",
  "bugs": {
    "url": "https://github.com/MRDGH2821/Discord-Ban-Utils-Bot/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MRDGH2821/Discord-Ban-Utils-Bot.git"
  },
  "license": "MIT",
  "author": {
    "name": "MRDGH2821",
    "url": "https://github.com/MRDGH2821"
  },
  "contributors": [
    {
      "name": "AroLeaf",
      "url": "https://github.com/AroLeaf"
    }
  ],
  "scripts": {
    "build": "tsc",
    "dev": "npm run build && npm run dev:start",
    "dev:start": "node --env-file .env dist/index.js",
    "docker:app": "docker compose up -d --build app",
    "format": "prettier --write .",
    "format-lint": "npm run format && npm run lint",
    "generate": "sapphire generate",
    "lint": "eslint --fix . --ignore-path .gitignore",
    "mega-lint": "npx -y mega-linter-runner --flavor javascript",
    "prepare": "is-ci || husky install",
    "start": "node dist/index.js",
    "watch": "tsc -w",
    "watch:start": "tsc-watch --onSuccess \"npm run dev:start\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --fix --ignore-path .gitignore"
    ],
    "*": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@sapphire/decorators": "^6.0.2",
    "@sapphire/discord.js-utilities": "^7.0.2",
    "@sapphire/framework": "^4.7.2",
    "@sapphire/plugin-logger": "^3.0.6",
    "@sapphire/plugin-subcommands": "^5.0.0",
    "@sapphire/shapeshift": "^3.9.3",
    "@sapphire/time-utilities": "^1.7.10",
    "@sapphire/utilities": "^3.13.0",
    "colorette": "^2.0.20",
    "discord.js": "^14.14.1",
    "dpaste-ts": "^2.0.0",
    "enhanced-ms": "^2.3.0",
    "firebase-admin": "^11.11.0",
    "tslib": "^2.6.2",
    "yaspr": "^1.0.2"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@sapphire/cli": "^1.6.1",
    "@sapphire/ts-config": "^5.0.0",
    "@types/node": "^20.9.0",
    "@types/ws": "^8.5.9",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "prettier-config-mrdgh2821": "^3.0.0",
    "prettier-plugin-packagejson": "^2.4.6",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=20.6.0"
  },
  "engineStrict": true
}
