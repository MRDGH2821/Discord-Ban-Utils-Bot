      "name": "AroLeaf",
      "url": "https://github.com/AroLeaf"
    }
  ],
  "scripts": {
    "build": "tsc",
    "dev": "npm run build && npm run dev:start",
    "dev:start": "node -r dotenv/config dist/index.js",
    "docker:app": "docker compose up -d --build app",
    "format": "prettier --write .",
    "generate": "sapphire generate",
    "lint": "eslint --fix . --ignore-path .gitignore",
    "mega-lint": "npx mega-linter-runner --flavor javascript",
    "sapphire": "sapphire",
    "start": "node dist/index.js",
    "watch": "tsc -w",
    "watch:start": "tsc-watch --onSuccess \"npm run dev:start\""
  },
  "dependencies": {
    "@sapphire/decorators": "^6.0.2",
    "@sapphire/discord.js-utilities": "^7.0.1",
    "@sapphire/framework": "^4.5.1",
    "@sapphire/plugin-logger": "^3.0.5",
    "@sapphire/plugin-subcommands": "^4.0.2",
    "@sapphire/shapeshift": "^3.9.2",
    "@sapphire/time-utilities": "^1.7.10",
    "@sapphire/utilities": "^3.13.0",
    "colorette": "^2.0.20",
    "discord.js": "^14.11.0",
    "dpaste-ts": "^2.0.0",
    "enhanced-ms": "^2.3.0",
    "firebase-admin": "^11.10.1",
    "tslib": "^2.6.1",
    "yaspr": "^1.0.2"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@sapphire/cli": "^1.6.1",
