    "@sapphire/shapeshift": "^3.9.2",
    "@sapphire/utilities": "^3.12.0",
    "colorette": "^2.0.20",
    "discord.js": "^14.11.0",
    "dpaste-ts": "^2.0.0",
    "yaspr": "^1.0.2"
  },
  "devDependencies": {
    "@sapphire/cli": "^1.6.1",
    "@sapphire/ts-config": "^4.0.0",
    "@types/node": "^18.16.18",
