{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "airbnb-ts-prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": ["./tsconfig.json"]
  },
  "rules": {
    "class-methods-use-this": [
      "error",
      {
        "exceptMethods": [
          "printBanner",
          "run",
          "chatInputRun",
          "autocompleteRun"
        ]
      }
    ]
  }
}
