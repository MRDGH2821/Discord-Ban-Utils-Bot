{
  "gitignore": true,
  "ignore": [
    "./src/listeners/**",
    "**.lock**",
    "**/.git/**",
    "**/.github/**",
    "**/.idea/**",
    "**/.rbenv/**",
    "**/.venv/**",
    "**/.yarn/**",
    "**/*.svg",
    "**/*cache*/**",
    "**/dist/**",
    "**/node_modules/**",
    "**/report/**",
    "CHANGELOG.md"
  ],
  "reporters": ["html", "markdown"],
  "threshold": 0,
  "ignorePattern": [
    "regist.*|name:.*|description:.*|dm.*|defaultMem.*",
    "guild.*|.*SERVER_ONLY",
    "reason.*|convict.*",
    "guild.*|.*convict.*",
    "permission.*|.*NOT_PERMITTED",
    "return.*reply\\(.*",
    "ephemeral.*",
    "guild.*|convict.*|ephemeral.*|return.*reply.*",
    "guild.*|content.*|ephemeral.*|return.*reply.*|.*permission.*",
    "container.*|name.*|piece.*|store.*"
  ]
}
